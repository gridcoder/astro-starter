---
import MarkdownLayout from "@layouts/MarkdownLayout.astro"
import { Content, frontmatter } from "./content.mdx"
import FloatingTOC from "../../components/FloatingTOC_v5.svelte"
import { toc } from "./toc.js"
import OrganizationsGrid from "@src/components/OrganizationsGrid.astro"
import organizations from "./organizations.json"
---

<MarkdownLayout frontmatter={frontmatter}>
  <FloatingTOC slot="toc" client:load toc={toc} />
  <Content />
  <OrganizationsGrid organizations={organizations} />
</MarkdownLayout>
